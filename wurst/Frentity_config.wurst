package Frentity_config
/** If the map's height is completely flat, set to false for improved performance */
@config public constant DYNAMIC_Z = true
/** Kills actors attached to Entities instead of removing them, when the entity is destroyed */
@config public constant KILL_ACTORS = true
/** Disables damage between allied units */
@config public constant DISABLE_TEAMATTACK = true
/** Shows a note when a teamattack occurs */
@config public constant SHOW_SQUARE = true